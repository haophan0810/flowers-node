doctype html
head
  meta(charset='utf-8')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
  meta(http-equiv='x-ua-compatible', content='ie=edge')
  title #{title}
  // Font Awesome
  link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.6.3/css/all.css')
  // Bootstrap core CSS
  link(href='css/bootstrap.min.css', rel='stylesheet')
  // Material Design Bootstrap
  link(href='css/mdb.min.css', rel='stylesheet')
  // Your custom styles (optional)
  link(href='css/style.min.css', rel='stylesheet')
  style.
    .map-container {
    overflow: hidden;
    padding-bottom: 56.25%;
    position: relative;
    height: 0;
    }
    .map-container iframe {
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    position: absolute;
    }
  // Main Navigation
block head-admin
body
  header
    // Navbar
    nav.navbar.fixed-top.navbar-expand-lg.navbar-light.white.scrolling-navbar
      .container-fluid
        // Brand
        a.navbar-brand.waves-effect(href='https://mdbootstrap.com/docs/jquery/', target='_blank')
          strong.blue-text MDB
        // Collapse
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
        // Links
        #navbarSupportedContent.collapse.navbar-collapse
          // Left
          ul.navbar-nav.mr-auto
            li.nav-item.active
              a.nav-link.waves-effect(href='#')
                | Home
                span.sr-only (current)
            li.nav-item
              a.nav-link.waves-effect(href='https://mdbootstrap.com/docs/jquery/', target='_blank') About MDB
            li.nav-item
              a.nav-link.waves-effect(href='https://mdbootstrap.com/docs/jquery/getting-started/download/', target='_blank') Free download
            li.nav-item
              a.nav-link.waves-effect(href='https://mdbootstrap.com/education/bootstrap/', target='_blank') Free tutorials
          // Right
          ul.navbar-nav.nav-flex-icons
            li.nav-item
              a.nav-link.waves-effect(href='https://www.facebook.com/mdbootstrap', target='_blank')
                i.fab.fa-facebook-f
            li.nav-item
              a.nav-link.waves-effect(href='https://twitter.com/MDBootstrap', target='_blank')
                i.fab.fa-twitter
            li.nav-item
              a.nav-link.border.border-light.rounded.waves-effect(href='https://github.com/mdbootstrap/bootstrap-material-design', target='_blank')
                i.fab.fa-github.mr-2
                | MDB GitHub
    // Navbar
    // Sidebar
    .sidebar-fixed.position-fixed
      a.logo-wrapper.waves-effect
        img.img-fluid(src='', alt='')
      .list-group.list-group-flush
        a.list-group-item.active.waves-effect(href='#')
          i.fas.fa-chart-pie.mr-3
          | Dashboard
        a.list-group-item.list-group-item-action.waves-effect(href='#')
          i.fas.fa-user.mr-3
          | Profile
        a.list-group-item.list-group-item-action.waves-effect(href='#')
          i.fas.fa-table.mr-3
          | Tables
        a.list-group-item.list-group-item-action.waves-effect(href='#')
          i.fas.fa-map.mr-3
          | Maps
        a.list-group-item.list-group-item-action.waves-effect(href='#')
          i.fas.fa-money-bill-alt.mr-3
          | Orders
    // Sidebar
  // Main Navigation
  // Main layout
block nav-admin
  
  footer.page-footer.text-center.font-small.primary-color-dark.darken-2.mt-4.wow.fadeIn
    // Call to action
    .pt-4
      a.btn.btn-outline-white(href='https://mdbootstrap.com/docs/jquery/getting-started/download/', target='_blank', role='button')
        | Download MDB
        i.fas.fa-download.ml-2
      a.btn.btn-outline-white(href='https://mdbootstrap.com/education/bootstrap/', target='_blank', role='button')
        | Start free tutorial
        i.fas.fa-graduation-cap.ml-2
    // /.Call to action
    hr.my-4
    // Social icons
    .pb-4
      a(href='https://www.facebook.com/mdbootstrap', target='_blank')
        i.fab.fa-facebook-f.mr-3
      a(href='https://twitter.com/MDBootstrap', target='_blank')
        i.fab.fa-twitter.mr-3
      a(href='https://www.youtube.com/watch?v=7MUISDJ5ZZ4', target='_blank')
        i.fab.fa-youtube.mr-3
      a(href='https://plus.google.com/u/0/b/107863090883699620484', target='_blank')
        i.fab.fa-google-plus.mr-3
      a(href='https://dribbble.com/mdbootstrap', target='_blank')
        i.fab.fa-dribbble.mr-3
      a(href='https://pinterest.com/mdbootstrap', target='_blank')
        i.fab.fa-pinterest.mr-3
      a(href='https://github.com/mdbootstrap/bootstrap-material-design', target='_blank')
        i.fab.fa-github.mr-3
      a(href='http://codepen.io/mdbootstrap/', target='_blank')
        i.fab.fa-codepen.mr-3
    // Social icons
    // Copyright
    .footer-copyright.py-3
      | &copy; 2018 Copyright:
      a(href='https://mdbootstrap.com/education/bootstrap/', target='_blank')
        | MDBootstrap.com
    // /.Copyright
  // /.Footer
  // SCRIPTS
  // JQuery
  script(type='text/javascript', src='js/jquery-3.3.1.min.js')
  // Bootstrap tooltips
  script(type='text/javascript', src='js/popper.min.js')
  // Bootstrap core JavaScript
  script(type='text/javascript', src='js/bootstrap.min.js')
  // MDB core JavaScript
  script(type='text/javascript', src='js/mdb.min.js')
  // Initializations
  script(type='text/javascript').
    // Animations initialization
    new WOW().init();
  // Charts
block footer-admin